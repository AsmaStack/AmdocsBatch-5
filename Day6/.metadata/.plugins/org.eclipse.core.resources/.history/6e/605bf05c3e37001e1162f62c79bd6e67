package listex;

import java.util.ArrayList;
import java.util.List;

public class TestListInterface {

	public static void main(String[] args) {

		List numbers=new ArrayList();
		
		numbers.add(10);
		numbers.add(20);
		numbers.add("Asma");
		numbers.add(12.5f);
		numbers.add(true);
		numbers.add("\b0");
		
		System.out.println(numbers);
		
		
		
		
	}

}
